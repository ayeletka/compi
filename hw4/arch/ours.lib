 L_SYMB_LOOP:
  CMP(R1, IMM(0));
  JUMP_EQ(L_SYMB_EXIT);
  CMP(IND(R2), '\n');
  JUMP_EQ(L_SYMB_NEWLINE);
  CMP(IND(R2), '\t');
