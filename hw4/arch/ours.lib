  CALL(PUTCHAR);
  DROP(2);
  JUMP(L_SYMB_LOOP_CONT);
 L_SYMB_OCT_CHAR:
  MOV(R0, IND(R2));
  MOV(R3, R0);
  REM(R3, IMM(8));
  PUSH(R3);
  DIV(R0, IMM(8));
  MOV(R3, R0);
