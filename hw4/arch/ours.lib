  DROP(1);
 L_SYMB_LOOP_CONT:
  INCR(R2);
  DECR(R1);
  JUMP(L_SYMB_LOOP);
 L_SYMB_EXIT:
  POP(R3);
  POP(R2);
  POP(R1);
  POP(FP);
  RETURN;

