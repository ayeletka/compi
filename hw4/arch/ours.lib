PRINT_R0:
	PUSH(FP);
	MOV(FP, SP);
	CMP(R0, IMM(T_VOID));
	JUMP_EQ(PRINT_R0_END);
	PUSH(R0);
	CALL(WRITE_SOB_HELPER);
	DROP(IMM(1));
	PUSH('\n');
	CALL(PUTCHAR);
	DROP(IMM(1));
PRINT_R0_END:
	JUMP(END);
END:
	POP(FP);
	RETURN;


